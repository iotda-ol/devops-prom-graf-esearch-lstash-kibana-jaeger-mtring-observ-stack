================================================================================
üì¶ DOCKER & DOCKER COMPOSE COMMANDS
================================================================================

# Container Management
docker ps                                    # List running containers
docker ps -a                                 # List all containers (including stopped)
docker ps --format "{{.Names}}"              # List container names only
docker stats                                 # Show resource usage statistics
docker logs <container>                      # View container logs
docker logs -f <container>                   # Follow container logs (real-time)
docker logs --tail 100 <container>           # Show last 100 log lines
docker logs --since 10m <container>          # Logs from last 10 minutes
docker exec -it <container> /bin/bash        # Interactive shell in container
docker exec -it <container> sh               # Shell (if bash not available)
docker inspect <container>                   # Detailed container info
docker top <container>                       # Running processes in container
docker port <container>                      # Port mappings
docker restart <container>                   # Restart container
docker stop <container>                      # Stop container
docker start <container>                     # Start stopped container
docker kill <container>                      # Force kill container
docker rm <container>                        # Remove stopped container
docker rm -f <container>                     # Force remove running container

# Image Management
docker images                                # List all images
docker pull <image>:<tag>                    # Pull image from registry
docker rmi <image>                           # Remove image
docker rmi -f <image>                        # Force remove image
docker image prune                           # Remove unused images
docker image prune -a                        # Remove all unused images
docker history <image>                       # Show image history
docker tag <image> <new-name>:<tag>          # Tag an image
docker save <image> > image.tar              # Export image to tar
docker load < image.tar                      # Import image from tar

# Docker Compose - Project Management
docker compose up                            # Start all services (foreground)
docker compose up -d                         # Start all services (background)
docker compose down                          # Stop and remove containers
docker compose down -v                       # Stop and remove containers + volumes
docker compose start                         # Start stopped services
docker compose stop                          # Stop running services
docker compose restart                       # Restart all services
docker compose pause                         # Pause services
docker compose unpause                       # Unpause services
docker compose ps                            # List containers in project
docker compose ps --format json              # List in JSON format
docker compose top                           # Show running processes

# Docker Compose - Service Management
docker compose up <service>                  # Start specific service
docker compose up -d <service>               # Start specific service (background)
docker compose stop <service>                # Stop specific service
docker compose restart <service>             # Restart specific service
docker compose logs <service>                # View service logs
docker compose logs -f <service>             # Follow service logs
docker compose logs -f --tail=100 <service>  # Follow last 100 lines
docker compose exec <service> bash           # Shell into service container
docker compose run <service> <command>       # Run one-off command in service

# Docker Compose - Configuration
docker compose config                        # Validate and view configuration
docker compose config --services             # List all services
docker compose config --volumes              # List all volumes
docker compose version                       # Show Compose version
docker compose -f custom.yml up              # Use custom compose file
docker compose pull                          # Pull all images defined in compose

# Docker Compose - Build
docker compose build                         # Build/rebuild services
docker compose build --no-cache              # Build without cache
docker compose build <service>               # Build specific service
docker compose up --build                    # Build and start

# Docker System Management
docker system df                             # Show disk usage
docker system prune                          # Remove unused data
docker system prune -a                       # Remove all unused data
docker system prune -a --volumes             # Remove all unused + volumes
docker system info                           # System-wide information
docker version                               # Docker version info

# Volume Management
docker volume ls                             # List volumes
docker volume inspect <volume>               # Inspect volume
docker volume rm <volume>                    # Remove volume
docker volume prune                          # Remove all unused volumes
docker volume create <name>                  # Create volume

# Network Management
docker network ls                            # List networks
docker network inspect <network>             # Inspect network
docker network rm <network>                  # Remove network
docker network prune                         # Remove unused networks
docker network create <name>                 # Create network

================================================================================
üî• PROMETHEUS COMMANDS & QUERIES
================================================================================

# Service Management
docker compose exec prometheus promtool check config /etc/prometheus/prometheus.yml
                                             # Validate Prometheus config
curl localhost:9090/-/healthy                # Check health
curl localhost:9090/-/ready                  # Check readiness
curl -X POST localhost:9090/-/reload         # Reload configuration

# PromQL Query Examples (via API or UI)
up                                           # All targets up/down status
up{job="prometheus"}                         # Specific job status
up{job="node-exporter"}                      # Node exporter status
node_cpu_seconds_total                       # CPU metrics
node_memory_Active_bytes                     # Active memory
rate(node_cpu_seconds_total[5m])             # CPU rate over 5 minutes
avg(rate(node_cpu_seconds_total[5m]))        # Average CPU rate
sum(rate(container_cpu_usage_seconds_total[5m])) by (name)
                                             # Container CPU by name
container_memory_usage_bytes                 # Container memory usage
process_resident_memory_bytes                # Process memory
http_requests_total                          # Total HTTP requests
rate(http_requests_total[5m])                # HTTP request rate
histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))
                                             # 95th percentile latency

# Common Aggregations
sum(metric)                                  # Sum all values
avg(metric)                                  # Average
max(metric)                                  # Maximum
min(metric)                                  # Minimum
count(metric)                                # Count
sum by (label) (metric)                      # Sum grouped by label
avg by (label) (metric)                      # Average grouped by label

# Time Functions
rate(metric[5m])                             # Per-second rate over 5 min
irate(metric[5m])                            # Instant rate
increase(metric[1h])                         # Increase over 1 hour
delta(metric[1h])                            # Delta over 1 hour

# HTTP API Queries
curl 'http://localhost:9090/api/v1/query?query=up'
                                             # Instant query
curl 'http://localhost:9090/api/v1/query_range?query=up&start=2024-01-01T00:00:00Z&end=2024-01-01T01:00:00Z&step=15s'
                                             # Range query
curl 'http://localhost:9090/api/v1/labels'  # Get all labels
curl 'http://localhost:9090/api/v1/label/__name__/values'
                                             # Get all metric names
curl 'http://localhost:9090/api/v1/targets' # Get all targets

