üîç ELASTICSEARCH COMMANDS
================================================================================

# Cluster Health & Info
curl localhost:9200                          # Cluster info
curl localhost:9200/_cluster/health          # Cluster health
curl localhost:9200/_cluster/health?pretty   # Pretty formatted
curl localhost:9200/_cluster/stats?pretty    # Cluster stats
curl localhost:9200/_nodes/stats?pretty      # Node stats
curl localhost:9200/_cat/nodes?v             # Nodes overview
curl localhost:9200/_cat/health?v            # Health overview
curl localhost:9200/_cat/indices?v           # Indices overview
curl localhost:9200/_cat/shards?v            # Shards overview

# Index Management
curl localhost:9200/_cat/indices             # List all indices
curl -X GET localhost:9200/<index>           # Get index info
curl -X PUT localhost:9200/<index>           # Create index
curl -X DELETE localhost:9200/<index>        # Delete index
curl -X POST localhost:9200/<index>/_close   # Close index
curl -X POST localhost:9200/<index>/_open    # Open index
curl localhost:9200/<index>/_settings?pretty # Get index settings
curl localhost:9200/<index>/_mapping?pretty  # Get index mapping

# Document Operations
curl -X GET localhost:9200/<index>/_search?pretty
                                             # Search all documents
curl -X GET localhost:9200/<index>/_doc/<id> # Get document by ID
curl -X POST localhost:9200/<index>/_doc -H 'Content-Type: application/json' \
  -d '{"field":"value"}'                      # Index document
curl -X PUT localhost:9200/<index>/_doc/<id> -H 'Content-Type: application/json' \
  -d '{"field":"value"}'                      # Index with ID
curl -X DELETE localhost:9200/<index>/_doc/<id>
                                             # Delete document
curl -X POST localhost:9200/<index>/_update/<id> -H 'Content-Type: application/json' \
  -d '{"doc":{"field":"new_value"}}'          # Update document

# Search Queries
curl -X GET localhost:9200/<index>/_search?q=field:value
                                             # Simple query string
curl -X GET localhost:9200/_search?pretty -H 'Content-Type: application/json' \
  -d '{"query":{"match_all":{}}}'             # Match all
curl -X GET localhost:9200/<index>/_search?pretty -H 'Content-Type: application/json' \
  -d '{"query":{"match":{"field":"value"}}}'  # Match query
curl -X GET localhost:9200/<index>/_search?pretty -H 'Content-Type: application/json' \
  -d '{"query":{"term":{"field":"value"}}}'   # Term query
curl -X GET localhost:9200/<index>/_search?pretty -H 'Content-Type: application/json' \
  -d '{"query":{"range":{"@timestamp":{"gte":"now-1h"}}}}'
                                             # Range query (last hour)

# Aggregations
curl -X GET localhost:9200/<index>/_search?pretty -H 'Content-Type: application/json' \
  -d '{"aggs":{"by_field":{"terms":{"field":"field_name"}}},"size":0}'
                                             # Terms aggregation
curl -X GET localhost:9200/<index>/_search?pretty -H 'Content-Type: application/json' \
  -d '{"aggs":{"avg_value":{"avg":{"field":"field_name"}}},"size":0}'
                                             # Average aggregation

# Index Templates
curl localhost:9200/_index_template?pretty   # List index templates
curl -X PUT localhost:9200/_index_template/<name> -H 'Content-Type: application/json' \
  -d @template.json                           # Create template

# Bulk Operations
curl -X POST localhost:9200/_bulk -H 'Content-Type: application/x-ndjson' \
  --data-binary @bulk_data.json               # Bulk index

